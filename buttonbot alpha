
import discord
from discord.ext commands import Bot
from discord
import random
from random.ext commands import random.randint
from random.ext commands import random.choice

Client = discord.Client() #Initialise Client 
client = commands.Bot(command_prefix = "&&") #Initialise client bot


@client.event 
async def on_ready():
    print("Bot is online and connected to Discord") #This will be called when the bot connects to the server

@client.event
async def on_message(message):
    if message.content == "push button":
        await client.send_message(message.channel, "https://www.boredpanda.com") 

